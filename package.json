{
  "name": "murderhornet",
  "productName": "murderhornet",
  "version": "1.0.11",
  "description": "Video streaming for the people",
  "repository": "kindlyops/murderhornet",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Kindly Ops, LLC",
    "email": "support@kindlyops.com"
  },
  "license": "Apache-2.0",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/Icon.icns",
        "osxSign": {
          "hardened-runtime": true,
          "gatekeeper-assess": false,
          "entitlements": "./src/entitlements.plist",
          "entitlements-inherit": "./src/entitlements.plist",
          "signature-flags": "library"
        },
        "osxNotarize": {
          "appleId": "elliot.murphy@gmail.com",
          "appleIdPassword": "@keychain:AC_PASSWORD"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "murderhornet"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ],
          "config": {
            "background": "./assets/ezra-jeffrey-comeau-4SVSaWbWq3U-unsplash.jpg",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "platforms": ["darwin", "linux", "win32"],
          "config": {
            "repository": {
              "owner": "kindlyops",
              "name": "murderhornet"
            },
            "prerelease": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.51",
    "@electron-forge/maker-deb": "6.0.0-beta.51",
    "@electron-forge/maker-dmg": "6.0.0-beta.51",
    "@electron-forge/maker-rpm": "6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "6.0.0-beta.51",
    "@electron-forge/maker-zip": "6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "6.0.0-beta.51",
    "@electron-forge/publisher-github": "6.0.0-beta.51",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "css-loader": "^3.0.0",
    "electron": "9.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "update-electron-app": "^1.5.0"
  }
}
